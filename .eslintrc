{
  "extends": ["prettier", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "parser": "babel-eslint",
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": "off",
    "curly": "error",
    "quotes": ["error", "single"],
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
